    "kryo": "workspace:^",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "typescript": "^5.3.3"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.mjs",
    "./*": "./lib/*.mjs"
