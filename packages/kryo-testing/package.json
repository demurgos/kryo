{
  "name": "kryo-testing",
  "version": "0.13.0",
  "description": "Helpers to test Kryo types and serializers",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://demurgos.github.io/kryo",
  "repository": {
    "type": "git",
    "url": "git://github.com/demurgos/kryo.git"
  },
  "type": "module",
  "files": [
    "./lib/",
    "./src/lib/"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "eslint --fix \"./src/**/*.ts\"",
    "build": "tsc --build src/lib/tsconfig.json",
    "prepack": "yarn run build"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "dependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "kryo": "workspace:^",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "typescript": "=4.7.0-beta"
  }
}
