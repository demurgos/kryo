{
  "name": "kryo-testing",
  "version": "0.14.2",
  "description": "Helpers to test Kryo types and serializers",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://demurgos.github.io/kryo",
  "repository": {
    "type": "git",
    "url": "git://github.com/demurgos/kryo.git"
  },
  "scripts": {
    "lint": "eslint \"./src/**/*.mts\"",
    "format": "eslint --fix \"./src/**/*.mts\"",
    "build": "tsc --build src/lib/tsconfig.json",
    "prepack": "yarn run build"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "dependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^10.0.6",
    "chai": "^5.0.0",
    "kryo": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "typescript": "^5.3.3"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.mjs",
    "./*": "./lib/*.mjs"
  },
  "files": [
    "./lib/**/*.(mjs|map|mts)",
    "./src/lib/"
