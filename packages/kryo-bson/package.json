{
  "name": "kryo-bson",
  "version": "0.11.2",
  "description": "BSON serializer for Kryo types",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://demurgos.github.io/kryo",
  "repository": {
    "type": "git",
    "url": "git://github.com/demurgos/kryo.git"
  },
  "type": "module",
  "files": [
    "./lib/",
    "./src/lib/"
  ],
  "exports": {
    "./lib/*": "./lib/*"
  },
  "scripts": {
    "build": "tsc --build src/lib/tsconfig.json",
    "pretest": "tsc --build src/test/tsconfig.json",
    "test": "c88 --reporter html --reporter text --reporter lcov-file mocha \"test/**/*.spec.js\"",
    "posttest": "yarn run lint",
    "watch": "gulp lib:watch",
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "eslint --fix \"./src/**/*.ts\""
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@types/bson": "^4.0.2",
    "bson": "^4.0.4",
    "incident": "^3.2.1",
    "kryo": "^0.11.0",
    "kryo-json": "^0.11.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "c88": "^0.3.4",
    "chai": "^4.2.0",
    "kryo-testing": "^0.11.0"
  },
  "c88": {
    "match": [
      "./lib/**/*.js"
    ]
  }
}
