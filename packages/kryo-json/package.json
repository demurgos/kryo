{
  "name": "kryo-json",
  "version": "0.12.1",
  "description": "JSON serializer for Kryo types",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://demurgos.github.io/kryo",
  "repository": {
    "type": "git",
    "url": "git://github.com/demurgos/kryo.git"
  },
  "type": "module",
  "files": [
    "./lib/",
    "./src/lib/"
  ],
  "exports": {
    "./lib/*": "./lib/*.js"
  },
  "scripts": {
    "build": "tsc --build src/lib/tsconfig.json",
    "test:build": "tsc --build src/test/tsconfig.json",
    "test": "yarn test:build && mocha \"test/**/*.spec.js\"",
    "posttest": "yarn run lint",
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "eslint --fix \"./src/**/*.ts\""
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "dependencies": {
    "incident": "^3.2.1",
    "kryo": "workspace:*",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "@types/unorm": "^1.3.28",
    "chai": "^4.3.4",
    "kryo-testing": "workspace:*",
    "mocha": "^9.0.1",
    "typescript": "^4.3.4",
    "unorm": "^1.6.0"
  }
}
